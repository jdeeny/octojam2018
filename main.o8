: main
  hires
  # load initial gamestate, which determines active systems, keymap, and ?


  vF := 1
  delay := vF
: gameloop
  # Process real time systems
  input_handler
  audio_handler


: systemloop
  # Process other systems
  system_handler

  vF := delay
  if vF != 0 then jump systemloop

: framedelay
  vF := 1
  delay := vF
jump gameloop
