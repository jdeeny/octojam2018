# Set I = address in registers

:macro reg-set-i-macro reg-hi reg-lo target {
	i := target
	save reg-hi - reg-lo
	0xF0 0x00 : target 0x00 0x00
	;
}


#: reg_load_i_PASS01 reg-set-i-macro PASS0 PASS1 reg_load_i_PASS01_target
