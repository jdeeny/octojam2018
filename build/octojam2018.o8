: input_handler_debounce
  v0 := 0
: input_handler_debounce_loop
  if v0 key then return
  v0 += 1
  if v0 != 16 then jump input_handler_debounce_loop
  i := input_state
  v0 := 0
  save v0 - v0
  ;

