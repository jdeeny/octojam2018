  i := audio_state_sustain
  save v1 - v1
  jump0 note_jumptable
: audio_handler_next_note_return
  #:breakpoint i=notedata
  audio
  i := music_data_ptr
  load v0 - v1
  math_add_u16_u8imm v0 v1 2
  save v0 - v1
  jump audio_handler_play_note


