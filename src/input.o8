:calc INPUT_TABLE_END { 0xFF }

: input_handler
  i := long input_table    # maybe change this with code modification?
: input_handler_loop
  load v1     # Load = v0 = handler tetra symbol, v1 = key
  if v0 == INPUT_TABLE_END then return
  if v0 key then tetra_exec     # tetra_exec expects the sumbol in v0
  jump input_handler_loop
